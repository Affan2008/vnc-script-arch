#!/bin/bash

print_usage() {
    echo
    echo "Usage $0"
    echo
    echo "nothing..."
    echo
}

argument="$1"
case $argument in
    awesome)
        "/usr/local/bin/start/awesome"
        ;;
    blackbox)
        "/usr/local/bin/start/blackbox"
        ;;
    bspwm)
        "/usr/local/bin/start/bspwm"
        ;;
    enlightenment)
        "/usr/local/bin/start/enlightenment"
        ;;
    fluxbox)
        "/usr/local/bin/start/fluxbox"
        ;;
    herbstluftwm)
        "/usr/local/bin/start/herbstluftwm"
        ;;
    i3)
        "/usr/local/bin/start/i3"
        ;;
    icewm)
        "/usr/local/bin/start/icewm"
        ;;
    jwm)
        "/usr/local/bin/start/jwm"
        ;;
    kwin)
        "/usr/local/bin/start/kwin"
        ;;
    lwm)
        "/usr/local/bin/start/lwm"
        ;;
    marco)
        "/usr/local/bin/start/marco"
        ;;
    metacity)
        "/usr/local/bin/start/metacity"
        ;;
    mutter)
        "/usr/local/bin/start/mutter"
        ;;
    mwm)
        "/usr/local/bin/start/mwm"
        ;;
    no_wm)
        "/usr/local/bin/start/no_wm"
        ;;
    notion)
        "/usr/local/bin/start/notion"
        ;;
    openbox)
        "/usr/local/bin/start/openbox"
        ;;
    pekwm)
        "/usr/local/bin/start/pekwm"
        ;;
    qtile)
        "/usr/local/bin/start/qtile"
        ;;
    ratpoison)
        "/usr/local/bin/start/ratpoison"
        ;;
    spectrwm)
        "/usr/local/bin/start/spectrwm"
        ;;
    twm)
        "/usr/local/bin/start/twm"
        ;;
    ukwm)
        "/usr/local/bin/start/ukwm"
        ;;
    xfwm4)
        "/usr/local/bin/start/xfwm4"
        ;;
    stop)
        "/usr/local/bin/start/stop"
        ;;
    xfce)
        "/usr/local/bin/start/xfce"
        ;;
    mate)
        "/usr/local/bin/start/mate"
        ;;
esac
